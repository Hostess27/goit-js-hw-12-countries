{{#each this}}
<h2 class="country__title">{{name.common}}</h2>
<div class="country__info">
    <p>Capital: <span class="regular-font">{{capital}}</span></p>
    <p>Population: <span class="regular-font">{{population}}</span></p>
    <ul class="languages-list">Languages:
        {{#each languages}}
        <li class="">{{''}}</li>
        {{/each}}
    </ul>
</div>
<img class="country__flag" src="{{flags.png}}" alt="Flag of {{name}}" width="200">

{{/each}}